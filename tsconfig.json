{
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"noEmit": true,
		"target": "ES2022",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"noCheck": true,
		"paths": {
		}
	},
	"include": [
		"**/*.ts",
		"**/*.mts"
	]
}